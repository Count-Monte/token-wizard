{
  "name": "oracles",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "concurrently": "^3.5.0",
    "gulp": "^3.9.1",
    "gulp-add-src": "^0.2.0",
    "gulp-concat": "^2.6.1",
    "gulp-include": "^2.3.1",
    "gulp-order": "^1.1.1",
    "gulp-uglify": "^2.0.0",
